#include <stdio.h>
int main()
{
	int a[5][5], b[5][5], c[5][5], d[5][5], i, j;
	printf("输入矩阵a\n");
	for (i = 1; i <= 5; i++)
		for (j = 1; j <= 5; j++)
			scanf_s("%d", &a[i][j]);
	printf("输入矩阵b\n");
	for (i = 1; i <= 5; i++)
		for (j = 1; j <= 5; j++)
			scanf_s("%d", &b[i][j]);
	//运算
	for (i = 1; i <= 5; i++)
		for (j = 1; j <= 5; j++)
		{
			c[i][j] = a[i][j] + b[i][j];
			d[i][j] = a[i][j] - b[i][j];
		}
	//输出
	for (i = 1; i <= 5; i++)
		for (j = 1; j <= 5; j++)
		{
			printf("%d", c[i][j]);
			printf("\n");
		}
	
	for (i = 1; i <= 5; i++)
		for (j = 1; j <= 5; j++)
		{
			printf("%d", d[i][j]);
			printf("\n");
		}
	return 0;
}
