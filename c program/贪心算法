//g数组是孩子的胃口；s数组是食物的量
//见leetcode455

int cmp(int* a, int* b) {
    return *a - *b;
}

int findContentChildren(int* g, int gSize, int* s, int sSize) {
    qsort(g, gSize, sizeof(int), cmp);
    qsort(s, sSize, sizeof(int), cmp);
    int i=0,j=0;
    while(i<gSize&&j<sSize)
    {
            if(j<sSize&&g[i]<=s[j])
            {
                    i++;
            }
            j++;
    }
    
    return i;
}
