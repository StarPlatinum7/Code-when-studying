#include <stdio.h>
int main()
{
	//冒泡排序
	int a[10], i, j;
	int temp;
	//循环语句输入元素
	for (i = 1; i <= 10; i++)
		scanf_s("%d", &a[i]);
	for(i=0;i<=9;i++)//外循环为冒泡的次数
		for(j=1;j<=i;j++)//每一次的比较
			if (a[j] > a[j + 1])//判断并且换序（从小到大）
			{
				temp = a[j];
				a[j] = a[j + 1];
				a[j + 1] = temp;
			}
	for (i = 1; i <= 10; i++)//循环输出
		printf("%d   ", a[i]);
	return 0;

}
c中的排序函数
qsort(a, 1000, sizeof(int), cmp);
//其中cmp函数应写为：
int cmp(const void *a, const void *b)
{
    return *(int*)a - *(int*)b; //由小到大排序
    //return *(int *)b - *(int *)a; 由大到小排序
}
