	printf("today is %d", w);break;
	case(6):
	    d=d+151;
	    w=(d+4)%7;
	printf("today is %d", w);break;
	case(7):
	    d=d+181;
	    w=(d+4)%7;
	printf("today is %d", w);break;
	case(8):
	    d=d+212;
	    w=(d+4)%7;
	printf("today is %d", w);break;
	case(9):
	    d=d+243;
	    w=(d+4)%7;
	printf("today is %d", w);break;
	case(10):
	    d=d+273;
	    w=(d+4)%7;
	printf("today is %d", w);break;
	case(11):
	    d=d+304;
	    w=(d+4)%7;
	printf("today is %d", w);break;
	case(12):
	    d=d+334;
	    w=(d+4)%7;
